  "scripts": {
    "test": "mocha",
    "test-cov": "istanbul cover --no-default-excludes -x 'node_modules/**' -x '**/test/**' -x '**/tests/**' ./node_modules/.bin/_mocha --dir ./reports/coverage -- -R spec",
    "codecov": "istanbul cover --no-default-excludes -x 'node_modules/**' -x '**/test/**' -x '**/tests/**' ./node_modules/.bin/_mocha --dir ./reports/codecov/coverage --report lcovonly -- -R spec && cat ./reports/codecov/coverage/lcov.info | codecov && rm -rf ./reports/codecov"
  },
  "main": "./lib",
  "repository": {
    "type": "git",
    "url": "git://github.com/kgryte/express-router-status.git"
  },
  "keywords": [
