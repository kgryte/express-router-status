    "app",
    "mount",
    "routes",
    "status",
    "health",
    "check",
    "endpoint",
    "middleware",
    "mw",
    "get",
    "rest"
  ],
  "bugs": {
    "url": "https://github.com/kgyte/express-router-status/issues"
  },
  "dependencies": {
    "express": "^4.13.3"
  },
  "devDependencies": {
    "@kgryte/noop": "^1.0.0",
    "chai": "3.x.x",
    "codecov.io": "^0.1.5",
    "istanbul": "^0.3.0",
    "jshint": "2.x.x",
    "jshint-stylish": "2.x.x",
    "mocha": "2.x.x",
    "request": "^2.61.0"
  },
